{
    "name": "zhi-server-modules-blog-astro",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "packages/zhi-server-modules-blog-astro/src",
    "targets": {
        "build": {
            "outputs": ["dist/packages/zhi/server/blog-astro"],
            "executor": "@nxtensions/astro:build",
            "options": {}
        },
        "serve": {
            "executor": "@nxtensions/astro:dev",
            "options": {}
        },
        "preview": {
            "dependsOn": [
                {
                    "target": "build",
                    "projects": "self"
                }
            ],
            "executor": "@nxtensions/astro:preview",
            "options": {}
        },
        "check": {
            "executor": "@nxtensions/astro:check"
        },
        "sync": {
            "executor": "@nxtensions/astro:sync"
        },
        "repo": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node tools/scripts/repo.mjs zhi-server-modules-blog-astro {args.to}"
            }
        }
    },
    "tags": []
}
